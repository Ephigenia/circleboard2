{
  "name": "circleboard",
  "version": "2.0.0",
  "description": "tabular display of the latest builds on CircleCi",
  "keywords": [
    "circleci",
    "dashboard",
    "status-monitor",
    "status",
    "monitor",
    "board"
  ],
  "contributors": [
    {
      "name": "Marcel Eichner",
      "email": "marcel.eichner@ephigenia.de"
    }
  ],
  "author": "Ephigenia M. Eichner",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ephigenia/circleboard2.git"
  },
  "bugs": {
    "url": "https://github.com/ephigenia/circleboard2/issues"
  },
  "engines": {
    "node": "^6.9.1",
    "npm": "^3.10.8"
  },
  "scripts": {
    "changelog": "conventional-changelog -s -i CHANGELOG.md -p angular -r 0",
    "commitmsg": "conventional-changelog-lint -e",
    "lint": "eslint source/scripts",
    "postinstall": "node_modules/bower/bin/bower install;",
    "postversion": "git push && git push --tags",
    "prestart": "npm install",
    "preversion": "npm test",
    "start": "node_modules/http-server/bin/http-server source -p 8000 -c-1",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "bower": "1.8.0",
    "http-server": "0.9.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^1.2.0",
    "conventional-changelog-lint": "^1.1.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-google": "0.4.0",
    "eslint-plugin-angular": "^1.0.0",
    "eslint": "^3.11.1",
    "husky": "^0.11.9"
  }
}
